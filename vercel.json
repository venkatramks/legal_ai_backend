{
  "version": 2,
  "functions": {
    "backend/api/app.py": {
      "runtime": "@vercel/python@0.5.0",
      "memory": 1024,
      "maxDuration": 300
    },
    "backend/api/**/*.py": {
      "runtime": "@vercel/python@0.5.0",
      "memory": 1024,
      "maxDuration": 300
    }
  },
  "routes": [
    {
      "src": "^/(.*)",
      "dest": "backend/api/app.py"
    }
  ]
}
