{
  "version": 2,
  "functions": {
    "api/app.py": {
      "runtime": "@vercel/python@1.0.0",
      "memory": 1024,
      "maxDuration": 300
    },
    "api/**/*.py": {
      "runtime": "@vercel/python@1.0.0",
      "memory": 1024,
      "maxDuration": 300
    }
  },
  "routes": [
    {
      "src": "^/(.*)",
      "dest": "api/app.py"
    }
  ]
}
